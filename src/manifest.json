{
    "manifest_version": 2,
    "name": "AruTab",
    "version": "0",
    "description": "",
    "permissions": [
        "<all_urls>",
        "tabs",
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "lib/jquery.min.js",
                "content/content.js"
            ],
            "css": [
                "content/content.css"
            ],
            "run_at": "document_start"
        }
    ],
    "web_accessible_resources": [
        "tablist/tablist.html"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}